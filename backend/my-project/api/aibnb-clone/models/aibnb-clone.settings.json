      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "places": {
      "collection": "destination-categories",
      "via": "destination"
    },
    "pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "host": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "guest": {
      "type": "string"
    },
    "beds": {
      "type": "string"
    },
    "baths": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "amenities": {
